{
    "extends": [
        "standard",
        "plugin:react/recommended",
        "plugin:tailwindcss/recommended",
        "prettier"
    ],
    "rules": {
        "max-len": [2, 500],
        "no-multiple-empty-lines": [
            "error",
            {
                "max": 1,
                "maxEOF": 1
            }
        ],
        "object-curly-newline": 0
    }
}